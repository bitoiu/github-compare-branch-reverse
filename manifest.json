{
  "name": "GitHub Branch Compare Reverse",
  "version": "0.1.1",
  "manifest_version": 2,
  "description":
    "Chrome extension that enables one click branch reversal on compare views",
  "homepage_url": "https://github.com/bitoiu/github-compare-branch-reverse",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["tabs", "https://github.com/*"],
  "background": {
    "scripts": ["src/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/compare/*"],
      "js": ["lib/jquery-3.3.1.slim.min.js", "src/inject.js"]
    }
  ]
}
